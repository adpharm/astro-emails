{
  "name": "astro-emails",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "tw": "tailwindcss -c ./tailwind.config.mjs -i ./src/assets/_styles.css -o ./src/assets/_styles-dist.css",
    "postcss": "postcss ./src/assets/_styles.css -o ./src/assets/_styles-dist.css",
    "transformers": "npm run script -- ./transformers/index.ts",
    "dev": "concurrently \"astro dev\" \"npm run postcss -- --watch\"",
    "start": "npm run dev",
    "_build": "npm run script -- ./build.ts",
    "build": "astro check && npm run _build",
    "build-unsafe": "npm run _build",
    "preview": "astro preview",
    "astro": "astro",
    "script": "ts-node --esm --transpileOnly "
  },
  "dependencies": {
    "@astrojs/check": "^0.5.6",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.4.11",
    "posthtml": "^0.16.6",
    "posthtml-safe-class-names": "^4.0.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-mso": "^1.4.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.6.0",
    "color-shorthand-hex-to-six-digit": "^5.0.16",
    "concurrently": "^8.2.2",
    "email-comb": "^7.0.20",
    "glob": "^10.3.10",
    "html-crush": "^6.0.18",
    "juice": "^10.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-merge-longhand": "^6.0.5",
    "postcss-selector-parser": "^6.0.16",
    "posthtml-attrs-parser": "^1.0.1",
    "posthtml-extra-attributes": "^3.0.0",
    "posthtml-match-helper": "^2.0.0",
    "posthtml-mso": "^3.0.0",
    "posthtml-postcss-merge-longhand": "^3.0.0",
    "prettier": "^3.2.5",
    "string-remove-widows": "^4.0.20",
    "tailwind-merge": "^2.2.1",
    "ts-node": "^10.9.2",
    "zx": "^7.2.3"
  }
}
